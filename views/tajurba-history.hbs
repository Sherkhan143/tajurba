<style>
    .navbar-area {
        border-bottom: 1px solid #ccc;
    }

    .flex-wrapper {
        display: flex;
        flex-flow: row nowrap;
    }

    .single-chart {
        width: 33%;
        justify-content: space-around;
    }

    .circular-chart {
        display: block;
        margin: 0;
        max-width: 70%;
        max-height: 250px;
    }

    .circle-bg {
        fill: none;
        stroke: #eee;
        stroke-width: 3.8;
    }

    .circle {
        fill: none;
        stroke-width: 2.8;
        stroke-linecap: round;
        animation: progress 1s ease-out forwards;
    }

    @keyframes progress {
        0% {
            stroke-dasharray: 0 100;
        }
    }

    .circular-chart.orange .circle {
        stroke: var(--lava);
    }

    .circular-chart.green .circle {
        stroke: var(--black);
    }

    .circular-chart.blue .circle {
        stroke: var(--gunmetal);
    }

    .percentage {
        fill: #666;
        font-family: sans-serif;
        font-size: 0.5em;
        text-anchor: middle;
    }

    .history-content-wrapper {
        display: flex;
        flex-direction: column;
        row-gap: 40px;
    }
</style>


